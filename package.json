{
  "name": "barracks-cli",
  "version": "0.0.2",
  "description": "A CLI tool for Barracks",
  "main": "index.js",
  "scripts": {
    "start": "./bin/barracks.js",
    "lint": "jshint **/**.js",
    "test": "npm run lint && npm run coverage && npm run check-coverage",
    "coverage": "DEBUG=true istanbul cover ./node_modules/mocha/bin/_mocha **/*.spec.js -- -R spec",
    "check-coverage": "istanbul check-coverage --statement 90 --branch 70 --function 90",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "bin": {
    "barracks": "bin/barracks.js"
  },
  "contributors": [
    {
      "email": "brice@barracks.io",
      "name": "Brice Argenson"
    },
    {
      "email": "gregoire@barracks.io",
      "name": "GrÃ©goire Weber"
    },
    {
      "email": "simon@barracks.io",
      "name": "Simon Guerout"
    }
  ],
  "keywords": [
    "Barracks",
    "IoT",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:barracksiot/barracks-cli.git"
  },
  "engine": "node >= 6.0",
  "author": "contact@barracks.io",
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "2.9.0",
    "mkdirp": "0.5.1",
    "pjson": "1.0.7",
    "prettyjson": "1.1.3",
    "proxyquire": "1.7.10",
    "read": "1.0.7",
    "request": "2.78.0",
    "request-promise": "4.1.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-spies": "0.7.1",
    "coveralls": "2.11.15",
    "istanbul": "0.4.5",
    "jshint": "2.9.4",
    "mocha": "3.2.0",
    "mock-stdin": "0.3.1",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0"
  }
}
